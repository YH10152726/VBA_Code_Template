Sub 品名チェック()

    Dim ws As Worksheet
    Set ws = Worksheets("品目票")

    Dim lastRow As Long
    lastRow = ws.Cells(ws.Rows.Count, "B").End(xlUp).Row

    MsgBox "B列の最終行は：" & lastRow

    Dim i As Long
    For i = 2 To lastRow
        MsgBox "行" & i & " の品名は：" & ws.Cells(i, 2).Value
    Next i

End Sub